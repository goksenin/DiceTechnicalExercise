<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#ffffff" />
            <corners
                android:bottomRightRadius="26dp"
                android:bottomLeftRadius="26dp"
                android:topLeftRadius="26dp"
                android:topRightRadius="26dp" />
            <gradient
                android:startColor="#ffb400"
                android:endColor="#ffb400"
                android:angle="45"/>
        </shape>

        <stroke android:width="2dp" android:color="#424242" />
    </item>
</selector>
